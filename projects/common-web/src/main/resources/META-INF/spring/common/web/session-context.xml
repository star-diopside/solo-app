<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="sessionStoreFilter" class="jp.gr.java_conf.star_diopside.common.web.session.SessionStoreFilter">
        <property name="sessionStoreService" ref="sessionStoreService" />
    </bean>

    <bean class="jp.gr.java_conf.star_diopside.common.web.session.StoredHttpSessionDestroyedEventListener">
        <property name="sessionStoreService" ref="sessionStoreService" />
    </bean>

</beans>
